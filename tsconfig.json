{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": true,
    "allowJs": false,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "keyofStringsOnly": true,
    "outDir": "./dist/app",
  },
  "exclude": [
    "node_modules",
    "src/setupTests.ts",
    "./node_modules/**/*",
    "./dist"
  ],
  "include": [
    "./src/**/*",
    "@type"
  ]
}